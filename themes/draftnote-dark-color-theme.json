	"tokenColors": [
		{
			"name": "Markdown Uniform Style",
			"scope": [
				"text.html.markdown",
				"markup.bold.markdown",
				"markup.list.unnumbered.markdown",
				"entity.name.section.markdown",
				"markup.heading.markdown",
				"markup.italic.markdown",
				"markup.quote.markdown",
				"punctuation.definition.heading.markdown",
				"punctuation.definition.bold.markdown",
				"punctuation.definition.italic.markdown",
				"punctuation.definition.quote.begin.markdown",
				"punctuation.definition.list.begin.markdown"
			],
			"settings": {
				"foreground": "#ffffffe5",
				"fontStyle": ""
			}
		},
		{
			"name": "Markdown - Bold",
			"scope": "markup.bold.markdown",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffffffe5"
			}
		},
		{
			"name": "Markdown - Italic",
			"scope": "markup.italic.markdown",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffffffa8"
			}
		},
		{
			"name": "Markdown - Headings",
			"scope": [
				"markup.heading.markdown",
				"entity.name.section.markdown"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#ffffffe5"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#ffffffa8"
			}
		},
		{
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffffe5"
			}
		},
		{
			"scope": [
				"keyword",
				"punctuation.definition.keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#F0718A"
			}
		},
		{
			"scope": [
				"variable",
				"meta.definition.variable.js",
				"entity.name.function",
				"entity.name.class",
				"support.function",
				"support.class",
				"support.type",
				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#FFB442"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
				"string.template"
			],
			"settings": {
				"foreground": "#5AC6AD"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.other"
			],
			"settings": {
				"foreground": "#ffffffe5"
			}
		},
		{
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "#5AC6AD"
			}
		},
		{
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#FFB442"
			}
		},
		{
			"scope": [
				"string.regexp",
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#5AC6AD"
			}
		},
		{
			"scope": "punctuation.section.embedded",
			"settings": {
				"foreground": "#F0718A"
			}
		},
		{
			"scope": "invalid",
			"settings": {
				"foreground": "#F0718A",
				"fontStyle": "bold"
			}
		}
	]
}
